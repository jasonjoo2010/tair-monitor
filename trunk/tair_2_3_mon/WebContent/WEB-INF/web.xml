	<filter>
		<filter-name>ssoFilter</filter-name>
		<filter-class>com.alibaba.buc.sso.client.filter.SSOFilter</filter-class>
		<init-param>
			<param-name>APP_NAME</param-name>
			<param-value>direwolf</param-value>
		</init-param>
		<init-param>
			<param-name>SSO_CALLBACK_CLASS</param-name>
			<param-value>com.alibaba.buc.sso.client.handler.impl.BucSSOCallBack</param-value>
		</init-param>

		<init-param>
			<param-name>SSO_SERVER_URL</param-name>
			<param-value>https://login.alibaba-inc.com</param-value>
		</init-param>
		<init-param>
			<param-name>CLIENT_KEY</param-name>
			<param-value>dc21d4ea-cc00-42ed-b4cc-11bef5702917</param-value>
		</init-param>
		<init-param>
			<param-name>APP_PORT</param-name> <!--用于替换BACK_URL的port -->
			<param-value>9999</param-value>
		</init-param>
		<init-param>
			<param-name>CLIENT_KEY_ONLINE</param-name>
			<param-value>66b2fa71-b0f2-4255-94af-dd2e288a3a02</param-value>
		</init-param>
		<init-param>
			<param-name>EXCLUSIONS</param-name>
			<param-value>/js/*,/datePicker/*,*/ok.html,*/status.taobao,/login.htm,/logout.htm,*/images*,*/imgs/*,*/css/*,*/favicon.ico</param-value>
		</init-param>
		<init-param>
			<param-name>RETURN_USER_EXTEND_INFO</param-name>
			<param-value>true</param-value>
		</init-param>
		<init-param>
			<param-name>ENCODING</param-name>
			<param-value>GBK</param-value>
		</init-param>
	</filter>

	<filter-mapping>
		<filter-name>ssoFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
